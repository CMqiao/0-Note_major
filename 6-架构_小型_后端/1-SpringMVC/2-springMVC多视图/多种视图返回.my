	<li>&lt;%@&nbsp;page&nbsp;language=&quot;java&quot;&nbsp;import=&quot;java.util.*&quot;&nbsp;pageEncoding=&quot;utf-8&quot;%&gt;&nbsp;&nbsp;</li>
	<li>&lt;%&nbsp;&nbsp;</li>
	<li>String&nbsp;path&nbsp;=&nbsp;request.getContextPath();&nbsp;&nbsp;</li>
	<li>String&nbsp;basePath&nbsp;=&nbsp;request.getScheme()+&quot;://&quot;+request.getServerName()+&quot;:&quot;+request.getServerPort()+path+&quot;/&quot;;&nbsp;&nbsp;</li>
